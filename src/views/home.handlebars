{{! plantilla con products actuales para el home  '/'}}
<div class="product-container">
  {{#each products}}
    <div class="card">
      <h1>{{this.title}}</h1>
      <h2>ID: {{this.id}}</h2>
      <p>Descripci√≥n: {{this.description}}</p>
      <p>Precio: {{this.price}}</p>
